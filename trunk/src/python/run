#!/bin/bash

# Run Effort Sets
echo "********** RUNNING EFFORT SETS **********"
for FILENAME in arff/*.arff;
do
    echo $FILENAME
    time ./idea.py --cluster --train $FILENAME --equalfrequency --logX --m 1 --n 1 --q 6
done

# Run Defect Sets
echo "********** RUNNING DEFECT SETS **********"
for FILENAME in arff/defect/*.arff;
do
    echo $FILENAME
    time ./idea.py --cluster --train $FILENAME --equalfrequency --logX --m 1 --n 1 --q 20
done
